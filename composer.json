{
    "name": "masterflash-ru/stream",
    "description": "Разные ленты, например новостей",
    "type": "Service",
    "license": "BSD-3-Clause",
    "keywords": [
        "masterflash",
        "zendframework"
    ],
    "homepage": "https://masterflash.ru/",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.0",
        "zendframework/zend-cache": "^2.7",
        "zendframework/zend-paginator": "^2.7",
        "masterflash-ru/ado": "*",
        "masterflash-ru/storage": "*",
        "masterflash-ru/seolist": "*"
    },
    "autoload": {
        "psr-4": {
            "Stream\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stream\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev",
            "dev-develop": "1.1-dev"
        },
        "zf": {
            "component": "Stream",
            "config-provider": "masterflash-ru\\Stream\\ConfigProvider"
        }
    }
}
